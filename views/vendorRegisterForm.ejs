    <form class="" action="/vendorRegisterForm" method="post">
      <div class="form-group">
        <label>Vendor Name : </label>
         <input class="form-control" style="margin-bottom:20px" type="text" name="venName" value="" aria-describedby="basic-addon1" autocomplete="off">
         <br>
        <label>Address : </label>
